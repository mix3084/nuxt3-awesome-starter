{
	"root": true,
	"extends": [
		"@nuxtjs/eslint-config-typescript",
		"plugin:prettier/recommended"
	],
	"rules": {
		"vue/multi-word-component-names": "off",
		"vue/no-multiple-template-root": "off",
		"@typescript-eslint/no-unused-vars": "off",
		"indent": ["error", "tab"],
		"prettier/prettier": [
			"error",
			{
				"useTabs": true,
				"endOfLine": "auto"
			}
		]
	}
}
